# $Header: $

depend() {
	need localmount net
	keyword -timeout
}

start() {
	ebegin "Starting dkms"
	  dkms-gentoo --check
	eend $?
}

stop() {
	ebegin "Stopping dkms"
	  dkms-gentoo --db
	eend $?
}


